{
  "extends": "airbnb-base",
  "rules": {
    "jsx-a11y/label-has-for": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "react/forbid-prop-types": "off",
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "arrow-parens": "off",
    "no-nested-ternary": "off",
    "no-plusplus": "off"
  }
}
